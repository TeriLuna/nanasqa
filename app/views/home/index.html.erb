<!-- Features section -->
<div id="first" class="section-md">
	<div class="container">
		<div class="row align-items-center col-spacing-40">
			<div class="col-12 col-md-4 order-md-2 text-center">
				<i class = "fa-pulse"><img src="<%= asset_url('logo.png')%>"></i>
			</div>
			<div class="col-12 col-md-4 order-md-1 text-md-right">
				<!-- Icon text box 1 -->
				<div class="margin-bottom-30 icon-4xl">
					<i class="ti-star text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.stars')%></h6>
					<p><%= t('features.stars_text')%></p>
				</div>
				<!-- Icon text box 2 -->
				<div class="icon-4xl">
					<i class="fa fa-tree text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.tree')%></h6>
					<p><%= t('features.tree_text')%></p>
				</div>
			</div>
			<div class="col-12 col-md-4 order-md-3">
				<!-- Icon text box 3 -->
				<div class="margin-bottom-30 icon-4xl">
					<i class="fas fa-mountain text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.dunes')%></h6>
					<p><%= t('features.dunes_text')%></p>
				</div>
				<!-- Icon text box 4 -->
				<div class="icon-4xl">
					<i class="ti-map-alt text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.routes')%></h6>
					<p><%= t('features.routes_text')%></p>
				</div>
			</div>
		</div><!-- end row -->
		<div class="col-12 order-md-2 text-center margin-top-40">
			<%= link_to(t('footer.volunteer'), volunteers_path, class:"button button-lg button-radius button-outline-dark-2 margin-top-40")%>
		</div>
	</div><!-- end container -->
</div>
<!-- end Features section -->

