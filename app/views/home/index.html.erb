<!-- Home section -->
<div class="bg-dark owl-carousel owl-nav-overlay owl-dots-overlay" data-owl-nav="false" data-owl-dots="false" data-owl-items="1" data-owl-autoPlay="true">
	<%- ['nanasqa-1.jpg', 'nanasqa-2.jpg', 'nanasqa-3.jpg', 'nanasqa-4.jpg'].each do |image_name| %>
	  <div class="section-fullscreen bg-image", data-bg-src= "<%= asset_url(image_name)%>">
	    <div class="container text-center">
	      <div class="position-middle">
	        <div class="row">
	          <div class="col-12 padding-60">
	              <h1 class="display-2 font-family-tertiary font-weight-black">
	                <%= t('index.index_text')%>
	              </h1>
	              <a class="button button-lg button-radius button-outline-white-2 margin-top-30 margin-right-20" href="#first"><i class="fab fa-apple"></i><%= t('reservation.book_now')%></a>
	              <a class="button button-lg button-radius button-outline-white-2 margin-top-30" href="#first"><i class="fab fa-android"></i><%= t('reservation.fun')%></a>
	          </div>
	        </div>
	      </div>
        <div class="position-bottom">
    			<a class="icon-xl" href="#first">
    				<i class="ti-angle-down"></i>
    			</a>
    		</div>
	    </div>
	  </div>
	<%- end %>
</div>
<!-- end Home section -->

