<!-- Home section -->
<div class="bg-dark owl-carousel owl-nav-overlay owl-dots-overlay" data-owl-nav="false" data-owl-dots="false" data-owl-items="1" data-owl-autoPlay="true">
	<%- ['nanasqa-3.jpg', 'nanasqa-2.jpg', 'nanasqa-4.jpg'].each do |image_name| %>
	  <div class="section-fullscreen bg-image bg-image-gradient-top", data-bg-src= "<%= asset_url(image_name)%>">
	    <div class="bg-white-06">
		    <div class="container text-center">
		      <div class="position-middle">
		        <div class="row">
		          <div class="col-12 col-md-10 offset-md-1 col-lg-8 offset-lg-2">
		          	<div class="">
		              <h1 class="font-weight-light line-height-160 margin-0">
		                <%= t('index.index_text_html')%>
		              </h1>
		              <%= link_to ongs_path, class: "button button-lg button-radius button-white margin-top-30" do %>
		              	<i class="ti-heart"></i>
		              	<%= t('reservation.ong')%>
		              <%- end %>
  	              <%= link_to reservations_path, class: "button button-lg button-radius button-dark margin-top-30 margin-right-" do %>
                  	<i class="ti-bookmark-alt"></i>
                  	<%= t('reservation.book_now')%>
                  <%- end %>
		              <%= link_to volunteers_path, class: "button button-lg button-radius button-white margin-top-30" do %>
		              	<i class="ti-face-smile"></i>
		              	<%= t('reservation.volunteer')%>
		              <%- end %>
		          	</div>
		          </div>
		        </div>
		      </div>
	        <div class="position-bottom">
	    			<a class="icon-xl text-dark text-bold" href="#first">
	    				<i class="ti-angle-down"></i>
	    			</a>
	    		</div>
		    </div>
	    </div>
	  </div>
	<%- end %>
</div>
<!-- end Home section -->

<!-- Features section -->
<div id="first" class="section-md">
	<div class="container">
		<div class="row align-items-center col-spacing-40">
			<div class="col-12 col-md-4 order-md-2 text-center">
				<i class = "fa-spin"><img src="<%= asset_url('logo.png')%>"></i>
			</div>
			<div class="col-12 col-md-4 order-md-1 text-md-right">
				<!-- Icon text box 1 -->
				<div class="margin-bottom-30 icon-4xl">
					<i class="ti-star text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20">
						<%= t('features.stars')%>
					</h6>
					<p><%= t('features.stars_text')%></p>
				</div>
				<!-- Icon text box 2 -->
				<div class="icon-4xl">
					<i class="fa fa-tree text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.tree')%></h6>
					<p><%= t('features.tree_text')%></p>
				</div>
				<!-- Icon text box 3 -->
				<div class="margin-bottom-30 icon-4xl">
					<i class="ti-user text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20">
						<%= t('features.know_new_people')%>
					</h6>
					<p><%= t('features.know_new_people_text')%></p>
				</div>
			</div>

			<div class="col-12 col-md-4 order-md-3">
				<!-- Icon text box 1 -->
				<div class="margin-bottom-30 icon-4xl">
					<i class="fas fa-mountain text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.dunes')%></h6>
					<p><%= t('features.dunes_text')%></p>
				</div>
				<!-- Icon text box 2 -->
				<div class="icon-4xl">
					<i class="ti-map-alt text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.routes')%></h6>
					<p><%= t('features.routes_text')%></p>
				</div>
				<!-- Icon text box 3 -->
				<div class="margin-bottom-30 icon-4xl">
					<i class="fas fa-home text-dark"></i>
					<h6 class="font-family-tertiary font-small font-weight-normal uppercase margin-top-20"><%= t('features.relax_chill')%></h6>
					<p><%= t('features.relax_chill_text')%></p>
				</div>
			</div>
		</div><!-- end row -->
		<div class="col-12 order-md-2 text-center margin-top-40">
			<%= link_to(t('footer.volunteer'), volunteers_path, class:"button button-lg button-radius button-dark margin-top-40")%>
		</div>
	</div><!-- end container -->
</div>
<!-- end Features section -->

